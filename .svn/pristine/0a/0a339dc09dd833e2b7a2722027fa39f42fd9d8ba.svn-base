.container1 {
  width: 100%;
  height: auto;
  display: flex;
  font-size: 14px;
  color: #636572;
  flex-direction: column;
  .navList {
      width: 402px;
      height: 60px;
      display: flex;
      justify-content: space-between;
      flex-direction: row;
      .navItem {
          font-size: 16px;
          background: #eeeef2;
          border-radius: 8px 8px 0px 0px;
          width: 130px;
          color: #161e33;
          height: 60px;
          text-align: center;
          line-height: 60px;
          position: relative;
          cursor: pointer;
      }
      .active {
          border-radius: 8px 8px 0px 0px;
          position: relative;
          background: #fff;
      }
      .active::after {
          content: '';
          position: absolute;
          top: 0;
          left: 0;
          width: 130px;
          height: 6px;
          background: #fa483a;
          border-radius: 8px 8px 0px 0px;
      }
  }
  .content3 {
      width: 100%;
      height: auto;
      display: flex;
      flex-direction: row;
      justify-content: flex-start;
      position: relative;
      .box {
          display: flex;
          flex-direction: column;
          width: 788px;
          height: auto;
          border: none !important;
          background: #ffffff;
          box-sizing: border-box;
          padding: 40px 24px 0;
          .start {
              display: flex;
              justify-content: flex-start;
              .input1 {
                  width: 196px;
                  input {
                      width: 100% !important;
                  }
              }
              .search {
                  border: none;
                  color: #fff;
                  width: 60px;
                  height: 32px;
                  display: flex;
                  justify-content: center;
                  align-items: center;
                  font-size: 14px;
                  margin-top: 1px;
                  cursor: pointer;
                  background: #fa483a;
                  border-radius: 0px 4px 4px 0px;
                  margin-left: -2px;
                  z-index: 2;
              }
          }
          .bx1 {
              width: 100%;
              height: auto;
              display: flex;
              justify-content: center;
              flex-direction: row;
              flex-wrap: wrap;
              margin-top: 50px;
              position: relative;
              margin-bottom: 60px;
              .binx {
                  width: 300px;
                  height: 565px;
                  margin-right: 65px;
                  position: relative;
                  .bxCont {
                      position: absolute;
                      bottom: 38px;
                      left: 2px;
                      width: 297px;
                      height: 405px;
                      box-sizing: border-box;
                      padding: 24px 10px 33px;
                      display: flex;
                      justify-content: space-between;
                      flex-direction: row;
                      .item {
                          width: 64px;
                          height: 348px;
                          background: #dce6f8;
                          border-radius: 12px;
                          flex-direction: column;
                          justify-content: flex-start;
                          box-sizing: border-box;
                          justify-content: center;
                          align-items: center;
                          padding: 14px 5px;
                          cursor: pointer;
                          box-sizing: border-box;
                          .item1 {
                              width: 54px;
                              height: 26px;
                              margin-bottom: 2px;
                              .img2 {
                                  width: 100%;
                                  height: 100%;
                              }
                              .img2:last-child {
                                  margin-bottom: 0;
                              }
                          }
                      }
                      .active {
                          box-sizing: border-box;
                          border: 2px solid #1ec084;
                          .item1 {
                              margin-left: -1px;
                          }
                      }
                  }
                  .wendu {
                      position: absolute;
                      width: 58px;
                      height: 22px;
                      border-radius: 4px;
                      top: 8px;
                      right: 36px;
                      font-size: 12px;
                      display: flex;
                      align-items: center;
                      color: #16277a;
                      justify-content: center;
                  }
                  .brand {
                      position: absolute;
                      width: 160px;
                      height: auto;
                      overflow: hidden;
                      text-overflow: ellipsis;
                      white-space: nowrap;
                      left: 40px;
                      bottom: 23px;
                  }
                  .name {
                      position: absolute;
                      width: 130px;
                      height: auto;
                      overflow: hidden;
                      text-overflow: ellipsis;
                      white-space: nowrap;
                      left: 40px;
                      top: 10px;
                  }
                  .img1 {
                      width: 100%;
                      height: 100%;
                  }
                  .box1 {
                      position: absolute;
                      top: 38px;
                      left: 22px;
                      width: 250px;
                      height: 435px;
                      overflow-y: scroll;
                      padding-top: 6px;
                      .boxItem {
                          width: 236px;
                          height: 102px;
                          margin: 0 auto 4px;
                          background: #dce6f8;
                          border-radius: 10px;
                          position: relative;
                          cursor: pointer;
                          .boxItemImg {
                              width: 100%;
                              height: 100%;
                              position: absolute;
                              left: 0;
                              top: 0;
                              z-index: 0;
                          }
                      }
                      .boxShow {
                          width: 236px;
                          height: 102px;
                          background: #dce6f8;
                          border-radius: 10px;
                          margin: 0 auto 4px;
                          overflow: hidden;
                          position: relative;
                          z-index: 3;
                          .list1 {
                              width: 100%;
                              height: 100%;
                              max-height: 102px;
                              box-sizing: border-box;
                              padding: 5px 10px;
                              display: flex;
                              flex-direction: column;
                              .item {
                                  width: 16px;
                                  height: 12px;
                                  background: #ffffff;
                                  border-radius: 2px;
                                  margin-bottom: 4px;
                              }
                              .item:last-child {
                                  margin-bottom: 0;
                              }
                          }
                          .list2 {
                              width: 100%;
                              height: 100%;
                              box-sizing: border-box;
                              padding: 5px 10px;
                              display: flex;
                              justify-content: flex-start;
                              align-content: flex-start;
                              flex-wrap: wrap;
                              .item {
                                  width: 16px;
                                  height: 12px;
                                  background: #ffffff;
                                  border-radius: 2px;
                                  box-sizing: border-box;
                                  margin: 0 4px 4px 0;
                              }
                          }
                          .boxItemImg {
                              display: none;
                          }
                      }
                  }
              }
              .binx1 {
                  width: 350px;
                  height: 530px;
                  position: relative;
                  margin-right: 55px;
                  font-size: 14px;
                  .name {
                      position: absolute;
                      width: 170px;
                      overflow: hidden;
                      text-overflow: ellipsis;
                      white-space: nowrap;
                      top: 13px;
                      left: 47px;
                      color: #fff;
                  }
                  .brand {
                      position: absolute;
                      width: 170px;
                      overflow: hidden;
                      text-overflow: ellipsis;
                      white-space: nowrap;
                      bottom: 38px;
                      color: #fff;
                      left: 47px;
                  }
                  .wendu {
                      width: 58px;
                      height: 18px;
                      color: #fff;
                      display: -webkit-box;
                      display: -ms-flexbox;
                      display: flex;
                      align-items: center;
                      justify-content: center;
                      position: absolute;
                      top: 12px;
                      right: 30px;
                  }
                  .img2 {
                      width: 100%;
                      height: 100%;
                  }
                  .cont {
                      position: absolute;
                      width: 272px;
                      height: 410px;
                      top: 45px;
                      left: 48px;
                      .item {
                          height: 100px;
                          width: 100%;
                          box-sizing: border-box;
                          position: relative;
                          cursor: pointer;
                      }
                      .active {
                          border: 1px solid green;
                          opacity: 0.4;
                          border-radius: 4px;
                      }
                      .item::after {
                          content: '';
                          position: absolute;
                          bottom: 0;
                          left: 0;
                          width: 100%;
                          opacity: 0.3;
                          height: 12px;
                          background: linear-gradient(0deg, #7b7cb6 0%, #cbd2ee 100%);
                      }
                      .item:last-child::after {
                          display: none;
                      }
                  }
              }
              .binx2 {
                  width: 453px;
                  height: auto;
                  position: relative;
                  margin-right: 30px;
                  margin-top: 140px;
                  .img1 {
                      width: 100%;
                      height: auto;
                  }
                  .cont {
                      position: absolute;
                      top: 135px;
                      left: 50%;
                      transform: translateX(-50%);
                      width: 325px;
                      height: 144px;
                      box-sizing: border-box;
                      padding: 12px 0;
                      display: flex;
                      justify-content: flex-start;
                      flex-wrap: wrap;
                      .item {
                          width: 50px;
                          height: 50px;
                          background: #ffffff;
                          border-radius: 4px;
                          margin: 0 4px 13px 0;
                          cursor: pointer;
                          position: relative;
                          overflow: hidden;
                      }
                      .item:nth-child(6n + 6) {
                          margin-right: 0;
                      }
                      .item1 {
                          width: 36px;
                          height: 36px;
                          margin: 0 5px 12px 0;
                          position: relative;
                      }
                      .item1:nth-child(8n) {
                          margin-right: 0;
                      }
                      .name {
                          width: 100%;
                          text-align: center;
                          position: absolute;
                          font-size: 12px;
                          color: red;
                          top: 50%;
                          left: 50%;
                          transform: translate(-50%, -50%);
                      }
                      .img2 {
                          width: 100%;
                          height: 100%;
                      }
                  }
              }
              .jiazi {
                  width: 188px;
                  height: 562px;
                  background: #dbe6f8;
                  border-radius: 12px;
                  box-sizing: border-box;
                  padding: 20px 10px;
                  .jiaziCont {
                      height: 512px;
                      overflow-y: scroll;
                      .item1 {
                          display: flex;
                          align-items: center;
                          justify-content: center;
                          font-weight: bold;
                          font-size: 14px;
                          margin-bottom: 14px;
                          width: 168px;
                          height: 40px;
                          background: #ffffff;
                          border-radius: 4px;
                          cursor: pointer;
                      }
                      .item1:last-child {
                          margin-bottom: 0;
                      }
                      .active {
                          background: #f9dcda;
                          position: relative;
                      }
                      .active::after {
                          content: '';
                          position: absolute;
                          left: 0;
                          top: 0;
                          width: 6px;
                          height: 100%;
                          background: #fa483a;
                          border-radius: 3px 0px 0px 3px;
                      }
                  }
              }
          }
      }
      .box:nth-child(2) {
          margin-left: 50px;
      }
      .box:first-child::after {
          content: '';
          width: 42px;
          height: 123px;
          position: absolute;
          left: 788px;
          top: 50%;
          transform: translateY(-50%);
          background-image: url('../../assets/img/applyPng.png');
      }
  }
}
.container1 .content3 .box:nth-child(2) .bx1 .jiazi .jiaziCont .active {
  background: #e4f7f0;
}
.container1 .content3 .box:nth-child(2) .bx1 .jiazi .jiaziCont .active::after {
  background: #1ec084;
}
